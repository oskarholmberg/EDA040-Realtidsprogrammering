package lift;

public class Lift extends Thread{
	
	private Monitor m;
	
	public Lift(Monitor monitor){
		m = monitor;
	}
	
	public void run(){
		while(true){
			m.moveLift();
			m.haltLift();
			try {
				sleep(2000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			
		}
	}

}
