package lift;

public class Main {

	public static void main(String[] args) {
		LiftView lv = new LiftView();
		Monitor monitor = new Monitor(lv);
		Lift l = new Lift(monitor);
		for (int i = 0; i < 20; i++) {
			Person p = new Person(monitor);
			System.out.println(i);
			p.start();
		}
		l.start();
	}
}
